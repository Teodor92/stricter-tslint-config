{
    "name": "@strictr/tslint-config",
    "version": "1.3.0",
    "description": "A strict linting configuration for tslint.",
    "main": "tslint.js",
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "scripts": {
        "release": "standard-version"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:strictr/tslint-config.git"
    },
    "keywords": [
        "tslint",
        "tslint-config"
    ],
    "author": "Teodor Kurtev",
    "license": "MIT",
    "dependencies": {
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "5.2.1",
        "tslint-misc-rules": "3.5.1",
        "tslint-sonarts": "1.9.0"
    },
    "peerDependencies": {
        "tslint": "5.20.1"
    },
    "devDependencies": {
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "cz-conventional-changelog": "3.0.2",
        "husky": "4.0.10",
        "standard-version": "7.0.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
